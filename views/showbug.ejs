
<html>
    <link rel="stylesheet" href="/header.css">
    <link href="https://cdn.jsdelivr.net/npm/select2@4.1.0-rc.0/dist/css/select2.min.css" rel="stylesheet" />

    <style>
          #mainbtn{
            background-color: rgb(35, 11, 100);
            position:relative;
            top:25%;
            left:60%;
            color: white;
            border-radius: 2px;
            border-top: 2px;
            width: 200px;
            height:40px;

        }
        .mainfrm{
            display:block;
            position: relative;
            left:100px;
            top:20px;
            background-color:rgb(240, 241, 243);
            width:800px;
            height:600px;
            border-radius: 15px;
            border-style:groove;
            border-top: 4px;
            overflow-y: auto;
        }

         .data{
            padding-top: 13px;
            padding-left: 10px;
            padding-bottom: 10px;
            border-radius: 4px;
            border: 4px solid;
            border-top: 4px;
            border-left: 4px;
            border-right: 4px;
            border-color: rgb(212, 212, 204);
            font-family: 'Lucida Sans', 'Lucida Sans Regular', 'Lucida Grande', 'Lucida Sans Unicode', Geneva, Verdana, sans-serif;
        }

        .agents{
            position: absolute;
            top:10px;
            left:510px;
        }
        .nums{
            position: relative;
            top:110px;
            left:450px;
        }
     

        .bdy{
            position: relative;
            top:140px;
            right:200px;
        }
        
    </style>
            <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
            <script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>
            <script src="https://cdn.jsdelivr.net/npm/select2@4.1.0-rc.0/dist/js/select2.min.js"></script>



<script>
    $(document).ready(()=>{
        $("#next").on("click",()=>{

            $.get("/next",function(data,status){
                
                $(".data").remove()
                for(let i =0;i<data.length;i++){
                    let x = document.createElement("p");
                    x.setAttribute("class","data");
                    let n = document.createTextNode(data[i])
                    x.appendChild(n)
                    let main = document.querySelector(".mainfrm")
                    main.append(x)
                }

    
            })
        })
    })
</script>
    <body>

<div class="main">

            
    <p id="corner">File Space</p>
    <p onclick="location.href='/logout'" id="logout">Log Out</p>
    <div class="about">
    
        <p onclick="location.href='/profile'">Aakash</p>

        
    </div>


    </div>

    <div class="mainfrm">
            <p class="data"><%= data %> </p>
           
        
    </div>
    


    

    </body>
    </html>
    